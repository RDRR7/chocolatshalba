<div class="container">
  <% if !current_user.warehouse_role %>
    <div class="form-signup">
      <div class="row">
        <div class="col m12">
          <div class="card">
            <div class="card-content">
              <div class="page-header">
                <h1>Controles de ingreso</h1>
              </div>
              <table>
                <thead>
                  <tr>
                    <th>Recibido por</th>
                    <th>Entregado por</th>
                    <th>Fecha de Ingreso</th>
                  </tr>
                </thead>
                <tbody>
                  <% @entries.each do |entry| %>
                    <tr>
                      <td><%= entry.receivedBy %></td>
                      <td><%= entry.deliveredBy %></td>
                      <td><%= entry.entryDate %></td>
                      <td><%= link_to "Lotes", entry %></td>
                    </tr>
                  <% end %>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
